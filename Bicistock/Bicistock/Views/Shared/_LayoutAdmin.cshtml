@using Microsoft.AspNetCore.Http;
@{
    string activeUser;
    if (string.IsNullOrEmpty(Context.Session.GetString("activeUser")))
    {
        activeUser = "Inicia Sesión";

    }
    else
    {
        activeUser = "Hola, " + Convert.ToString(Context.Session.GetString("activeUser"));
    }
}
